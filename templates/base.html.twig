            }
            .loginhere-link {
                font-weight: 700;
                color: #222;
            }
            .container-fluid .login{
            width: 660px;
                position: relative;
                margin: 0 auto;
                }
            .register{
                background: #fff;
                border-radius: 10px;
                -moz-border-radius: 10px;
                -webkit-border-radius: 10px;
                -o-border-radius: 10px;
                -ms-border-radius: 10px;
